{
  "name": "vue-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate of vuejs with webpack and many more",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.config.js --env=dev",
    "script": "webpack --config webpack/webpack.config.js --env=dev",
    "watch": "webpack --progress --watch --config webpack/webpack.config.js --env=dev",
    "dist": "webpack --config webpack/webpack.config.js --env=prod",
    "serve": "http-server ./dist",
    "serve-dev": "npm run script && http-server ./dist",
    "serve-dist": "npm run dist && http-server ./dist",
    "deploy": "deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minnerwa/vue-boilerplate.git"
  },
  "author": "Minnerwa <potchara40@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Minnerwa/vue-boilerplate/issues"
  },
  "homepage": "https://github.com/Minnerwa/vue-boilerplate#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuetify": "^1.1.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-regenerator-runtime": "^6.5.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-hot-loader": "^1.4.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
}
